item_weight = 0.75
runes_weight = 0.25
item = c(15,25,15,20,15,15)
runes = c(5,5,5,5,5,5,5,5)
#testing
worse_items = c(1,1,1,1,1,1)
worse_runes = c(1,1,1,1,1,1,1)
best_runes = c(5,5,5,5,5,5,5)
best_items = c(25,20,20,20,20,20)
min_possible = 1 + (item_weight * sum(worse_items)) + (runes_weight * sum(worse_runes))
max_possible = 1 + (item_weight * sum(best_items)) + (runes_weight * sum(best_runes))
final_score = 1 + (item_weight * sum(item)) + (runes_weight * sum(runes))
normalized = (final_score - min_possible) / (max_possible - min_possible)
print(normalized)
